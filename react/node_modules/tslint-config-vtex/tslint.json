{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "never"],
    "no-console": false,
    "interface-name": false
  },
  "linterOptions": {
    "comment": "Exclude sibling node_modules automatically",
    "exclude": ["../../node_modules/**/*"]
  }
}